/**
  * @param side
  * @param? force_method
  * @param currency
  * @param currencySign
  * @param methods
  * @param methodID
  * @param showFeeDataEntry
  * @param amountID
  * @param? amount
  * @param? formattedAmount
  * @param percentFeeID
  * @param fixedFeeID
  * @param totalFeesID
  * @param netValueID
  * @param hideNetAmount
  * @param? verificationLevel
  * @param type
  * @param netAmountLabel
  */
{template .DepositWithdrawDialogContent}
  <form class="form-horizontal" data-deposit-status="prepare" data-uniform-control-holder-class="control-group">
  <fieldset>

    <input type="hidden" name="Currency" value="{$currency}">

    {if $side == 'broker'}
      <input id="{$amountID}" type="hidden" name="Amount" value="{$amount / 1e8}">
    {/if}

    <div class="error"></div>

    {if $type == 'withdrawal'}
      <div class="alert alert-warning text-left bitex-model"
           data-model-action="show_element"
           data-model-key-list="HasAnyPosition"
           data-model-formula="HasAnyPosition"
           style="display:none;">
      {msg desc="Warning message when the user has unconfirmed deposits."}
        You can't withdraw funds from the exchange while you have unconfirmed deposits.
      {/msg}
      </div>
    {/if}

    {if not $force_method}
      {if length($methods) > 1}
        <div class="col-lg-12 col-md-12 col-sm-12 control-group">
          <label class="control-label">{msg desc="method"}Method{/msg}</label>
          <div class="controls">
            <select id="{$methodID}" name="Method" class="withdraw-method-selector form-control">
              {foreach $method in $methods }
               <option value="{$method['method']}"
                    data-net-value="{$method['method']}_{$netValueID}"
                    data-percent-fee="{$method['method']}_{$percentFeeID}"
                    data-fixed-fee="{$method['method']}_{$fixedFeeID}">{$method['description']}</option>
              {/foreach}
            </select>
          </div>
        </div>
      {else}
        {foreach $method in $methods }
          <input  id="{$methodID}" type="hidden" name="Method" class="form-control"  value="{$method['method']}"
                data-net-value="{$method['method']}_{$netValueID}"
                data-percent-fee="{$method['method']}_{$percentFeeID}"
                data-fixed-fee="{$method['method']}_{$fixedFeeID}">
        {/foreach}
      {/if}
    {/if}

    <div class="withdraw-methods">
     {if length($methods) == 0 }
        {if $verificationLevel <= 2}
          <div class="alert alert-warning text-left">
            {msg desc="Error message when the user is not yet verified."}
              Due to anti-money laundry international laws and government regulations,
              deposits/withdrawals in the local currency are only available to verified customers.<br/>
              We are very sorry for the inconvenience.
            {/msg}
          </div>
          {if $verificationLevel == 0}
            <a href="" data-switch-view="verification">{msg desc="get verified message"} Verify your account {/msg}</a>
          {/if}
        {else}
          <div class="alert alert-danger text-center">
            {msg desc="Error message when the brokers doesn't have deposit methods"}We are not accepting deposits at this moment. We sorry for the inconvenience.{/msg}
          </div>
        {/if}
     {/if}
     {foreach $method in $methods }
        {if $force_method}
          {if $method['method'] == $force_method}

            {if $side == 'client'}
              <div class="control-group">
                <label class="control-label">{msg desc="amount"}Amount{/msg}</label>
                <div class="controls">
                  <div class="input-prepend">
                    <span class="add-on">{$currencySign}</span>
                    <input id="{$method['method']}_{$amountID}"
                        data-uniform-validators="required; validateNumber {if $method['limits'][$verificationLevel]['enabled']} {if $method['limits'][$verificationLevel]['min']}; validateMin {$method['limits'][$verificationLevel]['formatted_min_value'] }{/if}{if $method['limits'][$verificationLevel]['max']}; validateMax {$method['limits'][$verificationLevel]['formatted_max_value'] } {/if} {/if}"
                        data-uniform-type="number"
                        data-uniform-filters="positive_number"
                        type="text"
                        class="form-control withdraw-field input-small"
                        size="16"
                        value="{if $formattedAmount}{$formattedAmount}{/if}"
                        name="Amount">
                  </div>
                  {if $method['limits'][$verificationLevel]['enabled']}
                    <div>
                      <em>
                        <small>
                          {if $method['limits'][$verificationLevel]['max']}
                            {msg desc="amount help block"} Available: {$method['limits'][$verificationLevel]['formatted_max'] }{/msg}
                            <br>
                          {/if}
                          {if $method['limits'][$verificationLevel]['min']}
                            {msg desc="amount help block"} Minimum: {$method['limits'][$verificationLevel]['formatted_min'] } per transaction.  {/msg}
                          {/if}
                        </small>
                      </em>
                    </div>
                  {/if}
                </div>
              </div>
            {/if}


            {foreach $field in $method['fields'] }
              {if $field['side'] == $side}
                <div class="control-group">
                  <label class="control-label">{$field['label']}</label>
                  <div class="controls">
                    <input class="form-control withdraw-field"
                            data-uniform-validators="{$field['validator']}" type="{$field['type']}"
                            name="{$field['name']}"
                            label="{$field['placeholder']}"
                            value="{$field['value']}"/>
                  </div>
                </div>
              {/if}
            {/foreach}
            {if $side == 'client'}
              <div class="control-group" style="margin-bottom: 5px;">
                <div class="controls">
                  <label>{$method['disclaimer']}</label>
                  <input id="{$method['method']}_{$percentFeeID}" type="hidden" value="{$method['percent_fee']}">
                  <input id="{$method['method']}_{$fixedFeeID}" type="hidden" value="{$method['fixed_fee']}">
                </div>
              </div>

              <div class="control-group">
                <label class="control-label">{msg desc="Fee percentage label"}Fees:{/msg}</label>
                <div class="controls">
                  <label id="{$method['method']}_{$totalFeesID}" class="control-label" style="text-align: left;"></label>
                </div>
              </div>

              <div class="control-group" {if $hideNetAmount} style="display:none;" {/if} >
                <label class="control-label net-amount">{$netAmountLabel}:</label>
                <div class="controls">
                  <label id="{$method['method']}_{$netValueID}" class="control-label net-amount" style="text-align: left;"></label>
                  <input id="{$method['method']}_{$netValueID}_value" type="hidden" value="">
                </div>
              </div>

            {else}
              {if $side == 'broker'}
                <div class="control-group" style="margin-bottom: 5px;">
                  <div class="controls">
                    <label>{$method['disclaimer']}</label>
                  </div>
                </div>
                {if $showFeeDataEntry}
                  {call .FeesFormControls}
                    {param currencySign: $currencySign /}
                    {param percentFeeID: $method['method'] + '_' + $percentFeeID /}
                    {param percentFee: $method['percent_fee']  /}
                    {param fixedFeeID: $method['method']  + '_' +  $fixedFeeID /}
                    {param fixedFee: $method['fixed_fee'] /}
                    {param totalFeesID: $method['method']  + '_' +  $totalFeesID /}
                    {param netValueID: $method['method']  + '_' +  $netValueID /}
                  {/call}
                 {/if}
              {/if}
            {/if}
          {/if}
        {else}
          <div class="withdraw-method" data-withdraw-method="{$method['method']}" style="{if length($methods) > 1}display:none;{/if}">
            {if $side == 'client'}
              <div class="col-lg-6 col-md-6 col-sm-12 control-group">
                <div class="controls">
                  <label class="control-label">{msg desc="amount"}Amount{/msg}</label>
                  <div class="input-prepend">
                    <span class="add-on">{$currencySign}</span>
                    <input id="{$method['method']}_{$amountID}"
                        data-uniform-validators="required; validateNumber {if $method['limits'][$verificationLevel]['enabled']} {if $method['limits'][$verificationLevel]['min']}; validateMin {$method['limits'][$verificationLevel]['formatted_min_value'] }{/if}{if $method['limits'][$verificationLevel]['max']}; validateMax {$method['limits'][$verificationLevel]['formatted_max_value'] } {/if} {/if}"
                        data-uniform-type="number"
                        data-uniform-filters="positive_number"
                        value="{if $formattedAmount}{$formattedAmount}{/if}"
                        type="text"
                        class="form-control withdraw-field input-small"
                        size="16"
                        name="Amount" {if not isFirst($method)}disabled{/if}>
                  </div>
                  {if $method['limits'][$verificationLevel]['enabled']}
                    <div>
                      <em>
                        <small>
                          {if $method['limits'][$verificationLevel]['max']}
                            {msg desc="amount help block"} Maximum: {$method['limits'][$verificationLevel]['formatted_max'] }{/msg}
                            <br>
                          {/if}
                          {if $method['limits'][$verificationLevel]['min']}
                            {msg desc="amount help block"} Minimum: {$method['limits'][$verificationLevel]['formatted_min'] } per transaction.  {/msg}
                          {/if}
                        </small>
                      </em>
                    </div>
                  {/if}
                </div>
              </div>
            {/if}

            <div class="col-lg-6 col-md-6 col-sm-12 control-group">
              <div class="controls">
                <label class="control-label">{$method['disclaimer']}</label>
                <input id="{$method['method']}_{$percentFeeID}" type="hidden" value="{$method['percent_fee']}">
                <input id="{$method['method']}_{$fixedFeeID}" type="hidden" value="{$method['fixed_fee']}">
              </div>
              <div class="controls">
                <label class="requests-label control-label">{msg desc="Fee percentage label"}Fees:{/msg}</label>
                <label id="{$method['method']}_{$totalFeesID}" class="control-label" style="text-align: left;"></label>
              </div>
              <div {if $hideNetAmount} style="display:none;" {/if}>
                <div class="controls">
                    <label class="requests-label control-label net-amount">{$netAmountLabel}:</label>
                  <label id="{$method['method']}_{$netValueID}" class="control-label net-amount broker-confirm-deposit-net-amount" style="text-align: left;"></label>
                  <input id="{$method['method']}_{$netValueID}_value" type="hidden" value="">
                </div>
              </div>
            </div>

            {foreach $field in $method['fields'] }
              {if $field['side'] == $side}
                <div class="control-group">
                  <label class="control-label">{$field['label']}</label>
                  <div class="controls">
                    <input class="form-control withdraw-field"
                           data-uniform-validators="{$field['validator']}"
                           type="{$field['type']}"
                           name="{$field['name']}"
                           label="{$field['placeholder']}"
                           value="{$field['value']}"
                           {if not isFirst($method)}disabled{/if} />
                  </div>
                </div>
              {/if}
            {/foreach}
          </div>
        {/if}
     {/foreach}
    </div>


    <div class="control-group dlg-response-group">
      <label class="control-label dlg-response-group-label"></label>
      <div class="controls" class="dlg-response-group-value" >
      </div>
    </div>
    </fieldset>

  </form>
{/template}


